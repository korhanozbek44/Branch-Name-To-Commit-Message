{
  "name": "branch-name-to-commit-message",
  "displayName": "Branch Name To Commit Message",
  "description": "",
  "version": "0.0.1",
  "author": {
    "name": "Korhan Ã–ZBEK",
    "email": "korhanozbek44@gmail.com",
    "url": "https://korhanozbek.com"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "branch-name-to-commit-message.fill-commit-box",
      "title": "Branch To Commit"
    }],
    "configuration": {
      "type": "object",
      "title": "Branch Name To Commit Message",
      "properties": {
        "branch-name-to-commit-message.branchNameResolverRegex": {
          "type": "string",
          "default": "(.+)\/(.+)\/(.+)",
          "description": "Add brach name to commit message conversion regex"
        },
        "branch-name-to-commit-message.commitMessageConvention": {
          "type": "string",
          "default": "{{0}}({{1}}): {{2}}",
          "description": "How your commit message should looks like {{index of regex group}}"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
